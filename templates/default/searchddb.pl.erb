#!/usr/bin/perl
use strict;
use warnings;
use LWP::UserAgent;
use HTTP::Request::Common qw(GET);
use Crypt::SSLeay;

my $ua = LWP::UserAgent->new;

# Define user agent type
$ua->agent('Mozilla/8.0');

# Request object
my $req = GET 'https://duckduckgo.com';

# Make the request
my $res = $ua->request($req);

# Check the response
if ($res->is_success) {
  print $res->content;
} else {
  print $res->status_line . "\n";
}

exit 0;